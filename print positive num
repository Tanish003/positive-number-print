num = [12,-7,5,64,-14]
print("original numbers in the list: ",num)
new_num = list(filter(lambda x: x >0, num))
print("positive numbers in the list: ",new_num)


num = [12,14,-95,3]
print("original numbers in the list: ",num)
new_num = list(filter(lambda x: x >0, num))
print("positive numbers in the list: ",new_num)
